<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.devMode" value="true" />
    <constant name="struts.configuration.xml.reload" value="true" />
    
    <package name="egresados" extends="struts-default">
        <action name="*Registro" method="{1}" class="Controlador.RegistroAction">
            <result name="populate">/registroSistema.jsp</result>
            <result name="input">/registroSistema.jsp</result>
            <result name="success">/informacionPersonal.jsp</result>
        </action>
    </package>
</struts>